class Solution {
public:
    int calPoints(vector<string>& ops) {
        vector<string>arr;
        for(int i=0;i<ops.size();i++){
            if(ops[i]=='C'){
                arr.pop_back();
            }
            if(ops[i]=='D'){
                auto n=arr.back();
                n=2*n;
                arr.push_back(n);
            }
            if(ops[i]=='+'){
                int m=o
            }
        }
    }
};
