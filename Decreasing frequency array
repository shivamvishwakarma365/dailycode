#include <iostream>
#include<vector>
#include<unordered_map>
#include<algorithm>
using namespace std;
int main() {
 vector<int>arr;
 unordered_map<int,int>mp;
 arr={1,2,2,2,3,3,3,3,7,7,7,7,7};
for(int i:arr){
    mp[i]++;
}
sort(arr.begin(),arr.end(),[&](int a,int b){
    return mp[a]!=mp[b]?mp[a]>mp[b]:a>b;
});
for(int i:arr){
    cout<<i<<" ";
}
}
