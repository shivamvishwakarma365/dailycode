// Online C++ compiler to run C++ program online
#include <iostream>
#include<vector>
using namespace std;

int main() {
   string s;
   cout<<"enter a word to count its frequency"<<endl;
   cin>>s;
   vector<int>arr(26,0);
   for(int i=0;i<s.length();i++){
       arr[s[i]-'a']++;
   }
   for(int i:arr){
       cout<<i<<" ";
       }
    
}
